<view >
    <view >目标咨询师：{{counselor}}</view>
    <view class="de-wrapper">
        <view bindtap="change" wx:key="index" class="con-flex {{index === heighLightIndex ? 'heighLight' : ''}} {{item.count === 1 ? '' : 'canNotBook'}}" data-index="{{index}}" bindtap="check" wx:for='{{timeList}}'>
            <view class="p-wrapper">
                <view class='cl-name'>{{item.date}}</view>
                <view class='noBook' v-if="item.count === 0">不可预约</view>
                <view class='item-content'>{{item.time}}</view>
            </view>
        </view>
    </view>
    <view class="container">
    <mp-toptips msg="{{error}}" type="error" show="{{error}}"></mp-toptips>
    <view class="page" xmlns:wx="http://www.w3.org/1999/xhtml">
        <view class="page__bd">
            <mp-form id="form" rules="{{rules}}" models="{{formData}}">
                <mp-cells title="表单">
                    <mp-cell prop="mobile" title="手机号" ext-class=" weui-cell_vcode">
                        <input bindinput="formInputChange" data-field="mobile" class="weui-input" placeholder="请输入手机号"/>
                    </mp-cell>
                </mp-cells>
            </mp-form>

            <view class="weui-cells__title">想对我说点什么</view>
            <view class="weui-cells weui-cells_after-title">
                <view class="weui-cell">
                    <view class="weui-cell__bd">
                        <textarea class="weui-textarea" bindinput="bindTextChange" data-field="saySome" maxlength="200" placeholder="请输入文本" style="height: 4.3em" />
                        <view class="weui-textarea-counter">{{saySome.length}}/200</view>
                    </view>
                </view>
            </view>
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" bindtap="submitForm">确定</button>
            </view>
        </view>
        </view>
    </view>
</view>